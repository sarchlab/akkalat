	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python: Current File",
			"type": "python",
			"request": "launch",
			"program": "${file}",
			"console": "integratedTerminal",
			"justMyCode": true
		},
		{
			"name": "FIR",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${workspaceFolder}/model1_direct_conn/",
			"args": [
				"-timing",
				"-magic-memory-copy",
				"-benchmark=fir",
				"-report-all",
			],
			// "envFile": "${workspaceFolder}/../.env",
		},
		{
			"name": "PageRank",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${workspaceFolder}/model1",
			"args": [
				"-timing",
				"-magic-memory-copy",
				"-benchmark=pagerank",
				"-report-all",
			]
		}
	]
}